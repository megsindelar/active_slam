<launch>
  <node pkg="rviz2" exec="rviz2"
        args="-d $(find-pkg-share img_transform)/config/encoders.rviz"
        namespace="global"/>

  <node pkg="tf2_ros" exec="static_transform_publisher"
        args="--frame-id world --child-frame-id previous" />

  <node pkg="teleop_twist_keyboard" exec="teleop_twist_keyboard" name="teleop" output="screen" launch-prefix="xterm -e">
  </node>

  <node pkg="img_transform" exec="wheel_encoder">
  <param from="$(find-pkg-share img_transform)/config/diff_params.yaml"/>
  </node>

  <node pkg="img_transform" exec="registration">
  </node>

  <node pkg="img_transform" exec="active_learning">
  </node>

  <node pkg="img_transform" exec="se_sync">
  <param from="$(find-pkg-share img_transform)/config/diff_params.yaml"/>
  </node>

</launch>